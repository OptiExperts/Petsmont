<div class="find-us">
	<div class="container">
	<span>{{ section.settings.main_head}}</span>
	<br><br>
      <div class="fivehide">
      {% for block in section.blocks %}
			<img class="img-fluid" src="{{ block.settings.logo_image | img_url:master}}" alt="alternative">
      {% endfor %}
      </div>

	</div>
	</div>
{% schema %}
  {
    "name": "Home Find us",
    "settings": [
		{
			"type":"text",
			"id":"main_head",
			"label":"Heading",
			"default":"AS SEEN ON"
		}	
	],
    "blocks":[
      {
        "type" :"logo",
        "name" :"Add Logo",
        "limit":5,
        "settings":[
           {
             "type" :"image_picker",
             "id" :"logo_image",
             "label":"Logo"

           }
        ]
      }
    ],
  "presets" :[
      {
		"name" :"home_find_us_logo",
		"category":"Home Find us logo"
		}
	]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
