<div class="bg-img12">
	<!-- Testimonials -->
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 col-xl-6">
                    <div class="text-container light-brownbox">
                        <h2>{{ section.settings.collor_text_heading | upcase}}</h2>
						<div class="light-brown-line"><img class="img-fluid" src="https://cdn.shopify.com/s/files/1/0284/2391/3547/files/New_Project_18.png?v=1629831222" alt="alternative"></div>
                      {{ section.settings.collor_text_subheading}}  
                    </div> <!-- end of text-container -->
                </div> <!-- end of col -->
                <div class="col-lg-6 col-xl-6">
                    <div class="image-container">
                      {% if section.settings.collor_dcover_image != blank %}
                      <img class="img-fluid" src="{{section.settings.collor_dcover_image | img_url:'master'}}" alt="alternative">
                      {% else  %}
                        <img class="img-fluid" src="https://cdn.shopify.com/s/files/1/0284/2391/3547/files/pro-4.png?v=1629830952" alt="alternative">
                  	{% endif %}	  
                  </div> <!-- end of image-container -->
                </div> <!-- end of col -->
            </div> <!-- end of row -->
        </div>
	
	</div>

  
