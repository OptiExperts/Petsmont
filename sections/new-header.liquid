

<nav id="navbarExample" class="navbar navbar-expand-lg fixed-top navbar-light" aria-label="Main navigation">
    <div class="container-fluid">
      
       <button class="navbar-toggler p-0 border-0" type="button" id="navbarSideCollapse" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
    	<a class="navbar-brand logo-image" href="{{routes.root_url}}">
          {% if section.settings.add_logo != blank %}
             <img src="{{ section.settings.add_logo | img_url:'master'}}">
          {% else %}
          	<img src="https://cdn.shopify.com/s/files/1/0284/2391/3547/files/Logo-header.png?v=1629179286" alt="alternative"></a> 
    	  {% endif %}
      	
       <a class="d_cart_icon_wrap"  href="{{routes.cart_url}}"><img class="mobile-icon" src="https://cdn.shopify.com/s/files/1/0284/2391/3547/files/cart_5ba85df2-4f71-4580-8df4-8cff8c29e025.png?v=1629179398" alt="alternative">{% if cart.item_count > 0 %}<span class="cart__count">{{ cart.item_count }}</span>{% endif %}</a>
   
      
    	<div class="navbar-collapse offcanvas-collapse" id="navbarsExampleDefault">
    		<ul class="navbar-nav ms-auto navbar-nav-scroll header-menu-list">
              {% assign linkdata = linklists[section.settings.link_desktop] %}
              {% for link in linkdata.links %}
              <li class="nav-item {% if request.path == link.url%}actives {% endif %}">
                  <a class="nav-link" aria-current="page" href="{{link.url}}">{{link.title | upcase}}</a>
              </li>
              {% endfor %}

    </ul>
    	<span class="nav-item text-right">
            <img class="img-fluid header-icons search-icon" src="https://cdn.shopify.com/s/files/1/0284/2391/3547/files/srch.png?v=1629179343" alt="alternative">
          <a href="{{ routes.account_login_url }}"><img class="img-fluid header-icons" src="https://cdn.shopify.com/s/files/1/0284/2391/3547/files/user_1a6f50a6-a987-47eb-b3b7-6f71ec253f91.png?v=1629179372" alt="alternative"></a>
          <a id="dCartBtn" href="{{routes.cart_url}}"><img class="img-fluid header-icons" src="https://cdn.shopify.com/s/files/1/0284/2391/3547/files/cart_5ba85df2-4f71-4580-8df4-8cff8c29e025.png?v=1629179398" alt="alternative">{% if cart.item_count > 0 %}<span class="cart__count">{{ cart.item_count }}</span>{% endif %}</a>
        </span>
          
          
      <div class="social_icons">
        <ul>
          <li> <a href="{{ settings.social_facebook_url }}" target="_blank"<i class="fab fa-facebook-f"></i></a></li>
          <li> <a href="{{ settings.social_instagram_url }}" target="_blank"<i class="fab fa-instagram"></i></a></li>
          <li> <a href="{{ settings.social_amazon_url }}" target="_blank"<i class="fab fa-amazon"></i></a></li>
          
          
        </ul>
      </div>      
    </div>
	</div> 
</nav>
 <div class="header-search">
    <a href="#" class="header-search-toggle" aria-label="{{ 'general.accessibility_labels.close' | t | json }}">{% include 'svg-arrow-left' %}</a>

    <div class="header-search__content">
      <h2 class="">{{ 'layout.search.title' | t }}</h2>
      <form method="get" action="/search" autocomplete="off">
        <input type="hidden" name="type" value="{{ settings.search_type }}" />
        <input type="hidden" name="options[prefix]" value="last" />
        <div class="input-and-button-row">
          <div class="input-and-button-row__input-wrapper input-with-clear">
            <input class="input-and-button-row__input" type="text" name="q" placeholder="{{ 'layout.search.placeholder' | t | escape }}" aria-label="{{ 'layout.search.placeholder' | t | escape }}"  autocomplete="off" data-previous-value="">
            <a class="input-with-clear__clear" href="#">{% include 'svg-close' %}</a>
          </div>
          <button class="button input-and-button-row__button" type="submit">{{ 'layout.search.submit' | t }}</button>
        </div>
      </form>
      <div class="header-search__results-wrapper">
        <div class="header-search__placeholder">
          {%- for i in (1..5) -%}
            <div class="search-result search-result--placeholder">
              <div class="search-result__link">
                <div class="search-result__image"></div>
                <div class="search-result__detail">
                  <div class="search-result__title"></div>
                  <div class="search-result__price"></div>
                </div>
              </div>
            </div>
          {%- endfor -%}
        </div>
        <div class="header-search__results"></div>
      </div>

      {% if section.settings.search_menu_linklist != blank and linklists[section.settings.search_menu_linklist].links.size > 0 %}
        <div class="nav header-search__nav">
          {%- if section.settings.search_menu_title != blank -%}
            <h6 class="header-search__small-heading meta">{{ section.settings.search_menu_title }}</h6>
          {%- endif -%}

          <ul class="header-search__links">
            {% for link in linklists[section.settings.search_menu_linklist].links %}
            <li class="header-search__links__item"><a href="{{ link.url }}">{{ link.title }}</a></li>
            {% endfor %}
          </ul>
        </div>
      {% endif %}
    </div>
  </div>
<a href="#" class="focus-tint" aria-hidden="true" aria-label="{{ 'general.accessibility_labels.close' | t | json }}"></a>

<style>
  @media only screen 
  and (min-device-width: 375px) 
  and (max-device-width: 812px) 
  and (-webkit-min-device-pixel-ratio: 3) { 
    #navbarExample span.cart__count {
    height: 20px;
    width: auto;
    font-weight: 500;
    font-size: 11px;
    line-height: 20px;
    text-align: center;
    padding-left: 6px;
    padding-right: 5px;
    min-width: 20px;
    border-radius: 10px;
    right: unset;
    left: 15px !important;
    top: -10px !important;
}
  }
  
</style>

<script>
  jQuery(document).ready(function(){
    jQuery("#navbarExample .d_cart_btn").click(function(){
      console.log("clicked"); 
      
      console.log(jQuery("#dCartBtn")); 
    	jQuery("#dCartBtn img").trigger("click"); 
    })
  })
</script>
